{
	"semi": true,
	"jsxSingleQuote": true,
	"singleQuote": true,
	"trailingComma": "all",
	"printWidth": 80,
	"tabWidth": 2,
	"useTabs": true,
	"arrowParens": "always",
	"htmlWhitespaceSensitivity": "ignore",
	"plugins": [
		"prettier-plugin-astro",
		"prettier-plugin-svelte",
		"prettier-plugin-tailwindcss"
	]
}
